  <div>
    <% if flash[:error] %>
      <%= flash[:error] %>
    <% end %>
  </div>
  <div>
    <% if flash[:notice] %>
      <%= flash[:notice] %>
    <% end %>
  </div>
  
  <% if logged_in? %>
    <%= current_user.name %><br>
    <%= link_to "Log Out", logout_path, method: :delete %> 
  <% else %>
    <%= link_to "Sign Up", signup_path %>
    <%= link_to "Log In", login_path %>
  <%end%>